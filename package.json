    "gatsby-source-filesystem": "^2.3.37",
    "gatsby-transformer-remark": "^2.8.47",
    "gatsby-transformer-sharp": "^2.5.20",
    "gatsby-remark-smartypants": "^2.10.0",
    "gatsby-transformer-json": "^2.6.0",
    "is-internal-link": "^1.1.4",
    "modern-normalize": "^0.5.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.4.1",
    "prism-themes": "^1.5.0",
    "prismjs": "^1.16.0",
