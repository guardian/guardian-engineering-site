    "@guardian/src-foundations": "^2.8.2",
    "@guardian/src-icons": "^2.8.2",
    "@guardian/src-layout": "^2.8.2",
    "babel-plugin-styled-components": "^1.10.1",
    "classnames": "^2.2.6",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.25.0",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-canonical-urls": "^2.3.13",
    "gatsby-plugin-emotion": "^4.3.14",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.43",
